<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>ERT</display-name>
  <welcome-file-list>
    <welcome-file>Login.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <description></description>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.ert.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>LogoutServlet</display-name>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.ert.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/jsp/LogoutServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>EventReportingServlet</display-name>
    <servlet-name>EventReportingServlet</servlet-name>
    <servlet-class>com.ert.controller.EventReportingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EventReportingServlet</servlet-name>
    <url-pattern>/EventReportingServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EventResultsServlet</display-name>
    <servlet-name>EventResultsServlet</servlet-name>
    <servlet-class>com.ert.controller.EventResultsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EventResultsServlet</servlet-name>
    <url-pattern>/jsp/EventResultsServlet</url-pattern>
  </servlet-mapping>
</web-app>